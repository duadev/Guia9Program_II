<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - nextapp</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/favicon.ico" />

    <link href="~/css/theme.min.css" rel="stylesheet" />
</head>
<body>

    <nav class="p-2">
        <div class="container">
            <div class="row card">
                <div class="flush-nav">
                    <nav class="nav justify-content-md-center">
                        <a class="nav-link" href="/Home/Index">Home</a>
                        <a class="nav-link" href="/Docente/Index" asp-action="Index">Docentes</a>
                        <a class="nav-link" href="/Alumno/Index">Alumnos</a>
                        <a class="nav-link" href="/Materia/Index">Materia</a>
                        <a class="nav-link" href="/Nota/Index">Notas</a>
                    </nav>
                </div>
            </div>
        </div>
    </nav>

    <div class="container-fluid p-8">
        <main role="main">
            @RenderBody()
        </main>
    </div>


    <script src="~/js/validation.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

</body>
</html>



